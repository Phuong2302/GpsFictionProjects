<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.sdesimeur.android.gpsfiction.views.ZoneNameView
        android:id="@+id/textNameOfZone"
        style="@style/TabOneLineTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical" />

    <com.sdesimeur.android.gpsfiction.views.CompassView
        android:id="@+id/compassDirection"
        style="@style/TabCompassDraw"
        android:layout_width="match_parent"
        android:layout_gravity="center_horizontal" />

    <LinearLayout
        android:id="@+id/contentOther"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="horizontal">

        <com.sdesimeur.android.gpsfiction.views.ZoneDistance4CompassView
            android:id="@+id/textDistance"
            style="@style/TabCompassOther"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_horizontal" />
    </LinearLayout>
